{
  "include": [
    "./src/**/*"
  ],
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noImplicitAny": false
  }
}